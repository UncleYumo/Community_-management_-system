<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="539.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.communitymanagementsystem.ContactPropertyView_Controller">
    <TableView fx:id="tableView_myRecord" layoutX="14.0" layoutY="49.0" prefHeight="230.0" prefWidth="753.0">
      <columns>
          <TableColumn fx:id="tableColumn_requestID" prefWidth="69.0" text="记录序号"/>
          <TableColumn fx:id="tableColumn_description" prefWidth="292.0" text="需求描述"/>
          <TableColumn fx:id="tableColumn_phone" prefWidth="104.0" text="联系方式"/>
          <TableColumn fx:id="startTime" prefWidth="106.0" text="提交时间"/>
          <TableColumn fx:id="endTime" prefWidth="114.0" text="解决时间"/>
          <TableColumn fx:id="status" prefWidth="67.0" text="解决状态"/>
      </columns>
    </TableView>
    <Label layoutX="14.0" layoutY="14.0" text="我的提交记录">
        <font>
          <Font size="18.0"/>
        </font>
    </Label>
    <TextField fx:id="textField_description" layoutX="184.0" layoutY="344.0" prefHeight="41.0" prefWidth="513.0"/>
    <TextField fx:id="textField_phone" layoutX="184.0" layoutY="413.0" prefHeight="41.0" prefWidth="226.0"/>
    <Label layoutX="86.0" layoutY="353.0" text="需求描述">
        <font>
          <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Label layoutX="86.0" layoutY="422.0" text="联系方式">
        <font>
          <Font name="System Bold" size="18.0"/>
        </font>
    </Label>
    <Button fx:id="button_commitRequest" layoutX="441.0" layoutY="419.0" mnemonicParsing="false"
            onAction="#buttonAction_commitRequest" text="确认提交">
        <font>
          <Font name="System Bold" size="15.0"/>
        </font>
    </Button>
</AnchorPane>
